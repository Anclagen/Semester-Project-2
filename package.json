{
  "name": "de-clutter",
  "version": "0.6.1",
  "description": "An Auction site as part of a project assignment for Noroff",
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^11.2.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.1.5",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.55.0"
  },
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css",
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-e2e": "cypress open",
    "test-e2e-report": "cypress run --reporter mochawesome",
    "test-e2e-cli": "cypress run",
    "test-unit": "jest",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anclagen/Semester-Project-2.git"
  },
  "keywords": [
    "Bid",
    "Auction",
    "E-Commerce"
  ],
  "author": "Alexander Barrett",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Anclagen/Semester-Project-2/issues"
  },
  "homepage": "https://github.com/Anclagen/Semester-Project-2#readme",
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{scss,css,md}": "prettier --write"
  },
  "dependencies": {
    "bootstrap": "^5.2.2"
  }
}
